{
  "name": "aggregation-repository-provider",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/repository-provider.mjs",
  "module": "src/repository-provider.mjs",
  "description": "aggregates several repository providers into one",
  "keywords": [
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/repository-provider.mjs --section=API",
    "lint": "documentation lint src/repository-provider.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "dependencies": {
    "repository-provider": "^16.7.9"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "bitbucket-repository-provider": "^3.3.33",
    "c8": "^5.0.4",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "gitea-repository-provider": "^1.9.1",
    "github-repository-provider": "^7.4.20",
    "local-repository-provider": "^6.4.42",
    "markdown-doctest": "^1.0.0",
    "repository-provider-test-support": "github:arlac77/repository-provider-test-support",
    "semantic-release": "^15.13.26"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/aggregation-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/aggregation-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/aggregation-repository-provider#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
