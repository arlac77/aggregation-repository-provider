{
  "name": "aggregation-repository-provider",
  "version": "0.0.0-semantic-release",
  "main": "dist/index.js",
  "description": "aggregates several repository providers into one",
  "keywords": [
    "npm-package-template"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "jsdoc-babel": "-",
    "jsdoc-to-markdown": "-",
    "jsdoc": "-",
    "istanbul": "-",
    "mocha": "-",
    "chai": "-",
    "xo": "-",
    "cz-conventional-changelog": "-",
    "babel-plugin-transform-async-generator-functions": "-",
    "mocha-lcov-reporter": "-",
    "documentation": "^5.3.5",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.1",
    "ava": "^0.24.0",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^12.2.0",
    "travis-deploy-once": "^4.3.1"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/aggregation-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/aggregation-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/aggregation-repository-provider#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
